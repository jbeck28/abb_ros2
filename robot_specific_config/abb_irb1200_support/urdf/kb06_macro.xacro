<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:macro name="kb06" params="prefix">
        <link name="table">
            <collision>
                <origin rpy="0 0 0" xyz="0 0 0"/>
                <geometry>
                    <mesh filename="package://abb_irb1200_support/meshes/irb1200_5_90/scene/kb06_base_no_chuck.stl"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="0 0 0" xyz="0 0 0"/>
                <geometry>
                    <mesh filename="package://abb_irb1200_support/meshes/irb1200_5_90/scene/kb06_base_no_chuck.stl"/>
                </geometry>
                <xacro:material_abb_leaf_green/>
            </visual>
        </link>

        <link name="panels">
            <collision name="collision">
                <origin rpy="0 0 0" xyz="0 0 0"/>
                <geometry>
                    <mesh filename="package://abb_irb1200_support/meshes/irb1200_5_90/scene/panels_oversized.stl"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="0 0 0" xyz="0 0 0"/>
                <geometry>
                    <mesh filename="package://abb_irb1200_support/meshes/irb1200_5_90/scene/panels_oversized.stl"/>
                </geometry>
                <material name="">
                    <color rgba="0.5 0.5 0.5 0.2"/>
                </material>
                <!--xacro:material_abb_clear /-->
            </visual>
        </link>

        <link name="${prefix}extax_link">
            <inertial>
                <mass value="0.137"/>
                <origin xyz="0.6 0.150 0.0" rpy="0 0 0"/>
                <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
            </inertial>
            <collision>
                <origin rpy="0 0 0" xyz="0 0 0"/>
                <geometry>
                    <mesh filename="package://abb_irb1200_support/meshes/irb1200_5_90/scene/3RChuckAsm.stl"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="0 0 0" xyz="0 0 0"/>
                <geometry>
                    <mesh filename="package://abb_irb1200_support/meshes/irb1200_5_90/scene/3RChuckAsm.stl"/>
                </geometry>
                <xacro:material_abb_pebble_gray/>
            </visual>
        </link>

        <joint name="${prefix}base_link-table" type="fixed">
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <parent link="${prefix}base_link"/>
            <child link="${prefix}table"/>
        </joint>

        <joint name="${prefix}base_link-panels" type="fixed">
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <parent link="${prefix}base_link"/>
            <child link="${prefix}panels"/>
        </joint>
        <joint type="continuous" name="${prefix}ext_joint_1">
            <origin xyz="0.6096 0 0.100838" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <parent link="${prefix}base_link"/>
            <child link="${prefix}extax_link"/>
            <limit effort="1000" velocity="10.472"/>
            <dynamics damping="1.0" friction="1.0"/>
        </joint>
        <joint name="${prefix}ext_joint_1">
            <command_interface name="position">
            </command_interface>
            <command_interface name="velocity"/>
            <state_interface name="position">
                <param name="initial_value">0.0</param>
            </state_interface>
            <state_interface name="velocity"/>
        </joint>
    </xacro:macro>
</robot>